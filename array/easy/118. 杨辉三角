_doc = """
杨辉三角1:
输入: 5
输出:
[
     [1],
    [1,1],
   [1,2,1],
  [1,3,3,1],
 [1,4,6,4,1]
]
"""


def generator(num_rows):
    ans = [[1], [1, 1]]
    num_rows = int(num_rows)
    if num_rows == 0:
        return []
    elif num_rows == 1:
        return ans[:-1]
    elif num_rows == 2:
        return ans
    else:  # num_rows >= 3
        for row in range(2, num_rows):  # 执行几次生成列表的过程
            lst = [1]
            for i in range(0, len(ans[-1]) - 1):  # 推导中间的数，添加到列表
                num = ans[-1][i] + ans[-1][i + 1]
                lst.append(num)
            lst.append(1)
            ans.append(lst)
        return ans
